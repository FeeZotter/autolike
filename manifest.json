{
    "manifest_version": 2,
    "name": "autolikeWebtoonEpisodes",
    "version": "1.0",
  
    "description": "Likes all Webtoon episodes on entering. Nothing else, not even convinience features.",
  
    "icons": {
      "48": "icons/border-48.png"
    },
  
    "content_scripts": [
      {
        "matches": ["*://www.webtoons.com/*/*/*/*/viewer?title_no=*"],
        "js": ["like.js"]
      }
    ]
  }
  